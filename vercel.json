{
    "version": 2,
    "builds": [
        {
            "src": "backend/unibot_backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/unibot_backend/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}